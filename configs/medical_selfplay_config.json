{
  "medical_judge_model": "google/medgemma-4b-it",
  "medical_dataset": "data/medical_test_4way/train.jsonl",
  "pretrain": "Qwen/Qwen2.5-3B-Instruct",
  "critic_pretrain": null,
  "init_kl_coef": 0.01,
  "rollout_batch_size": 16,
  "max_len": 2048,
  "prompt_max_len": 1024,
  "generate_max_len": 1024,
  "seed": 42,
  "full_determinism": false,
  "ring_attn_size": 1,
  "ref_num_nodes": 1,
  "ref_num_gpus_per_node": 1,
  "reward_num_nodes": 1,
  "reward_num_gpus_per_node": 1,
  "actor_num_nodes": 1,
  "actor_num_gpus_per_node": 1,
  "critic_num_nodes": 1,
  "critic_num_gpus_per_node": 1,
  "colocate_actor_ref": false,
  "colocate_critic_reward": false,
  "colocate_all_models": false,
  "vllm_num_engines": 1,
  "vllm_tensor_parallel_size": 1,
  "enable_prefix_caching": false,
  "enforce_eager": false,
  "vllm_enable_sleep": false,
  "vllm_gpu_memory_utilization": 0.95,
  "local_rank": -1,
  "zero_stage": 2,
  "custom_configs": {
    "max_turns": 2,
    "reward_type": "medical_general_sum",
    "error_types": ["dosage", "diagnosis", "contraindication", "drug_interaction"],
    "direct_chat_no_cot": false,
    "no_attacker_turn": false,
    "no_assessor_turn": false
  }
}